<%- include('layouts/header.ejs') %>
<div class="container py-5">
  <div class="row m-5">
    <div class="col-md-12">
      <h1 class="text-dark text-center">Contact Us</h1>
    </div>
    <div class="col-md-12">
      <table class="table table-hovered table-stripped table-bordered">
        <thead>
          <tr>
            <th>SN</th>
            <th>Title</th>
            <th>Description</th>
            <th>Dialogue</th>
            <th>Address</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Link</th>
            <th>Quotes</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% contact.forEach((element,sn)=>{ %>
          <tr>
            <td><%=sn+1%></td>
            <td><%= element.title %></td>
            <td><%= element.description %></td>
            <td><%= element.dialogue %></td>
            <td><%= element.address %></td>
            <td><%= element.phone %></td>
            <td><%= element.email %></td>
            <td><%= element.link %></td>
            <td><%= element.quote1+element.quote2+element.quote3%></td>
            <td><%= element.image %></td>
            <td>
              <a href="/edit_contact/<%=element.id %>">
                <span class="bg-secondary px-3 rounded roun text-white"
                  >change</span
                >
              </a>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<%- include('layouts/footer.ejs') %>
