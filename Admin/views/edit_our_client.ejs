<%- include('layouts/header.ejs')%>


<form action="/update_our_client" method="post" enctype="multipart/form-data" class="p-5">
    <input type="hidden" name="client_id" value="<%=our_client_info[0].client_id%>">
    <div class="container bg-white text-dark">
        <div class="row">
            <div class="col-12 text-center">
                <h1 class="mt-5 text-dark">Update Our Clientt</h1>
            </div>
            <div class="col-12 mt-5">
                Client Info :
                 <input type="text" name="client_info" value="<%=our_client_info[0].client_info%>" class="form-control bg-white">
             </div>
            <div class="col-6 mt-5">
               Clinet Name :
                <input type="text" name="client_name" value="<%=our_client_info[0].client_name%>" class="form-control bg-white">
            </div>
            <div class="col-6 mt-5">
                Clinet Position :
                 <input type="text" name="client_pos" value="<%=our_client_info[0].client_pos%>" class="form-control bg-white">
             </div>
           <div class="col-12 text-center mt-5">
               <button class="btn btn-info btn-lg text-white">Save</button>
            </div>
           
        </div>
    </div>
</form>
<%- include('layouts/footer.ejs')%>
